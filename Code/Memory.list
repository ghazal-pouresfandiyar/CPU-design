0010_1000 //M[2] : AC = AC xor M[1000] = AC xor M[8] = AC xor 00001001
0011_1000 //M[3] : AC = M[1000] + M[1000] = M[8] + M[8] = 9 + 9
0100_1000 //M[4] : AC = M[1000] = 00001001 //read
0101_1000 //M[5] : M[1000] = AC //write
0110_1000 //M[6] : AC = ~M[1000] = ~(00001001) = 11110111
1000_1001 //M[7] : AC = AC + M[M[1001]] = AC + M[M[9]] = AC + M[8] = AC + 9
